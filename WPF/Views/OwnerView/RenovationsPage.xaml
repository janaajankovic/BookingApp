<Page x:Class="BookingApp.WPF.Views.OwnerView.RenovationsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BookingApp.WPF.Views.OwnerView"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="800"
      Title="RenovationsPage">

    <Grid>
        <Frame x:Name="ScheduleRenovationFrame"/>
        <Button Click="ScheduleRenovation_Click" Background="{StaticResource OwnerButtonColor2}" Content="Schedule a renovation" Width="150" Height="30" Margin="10,10,10,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Grid>
</Page>
